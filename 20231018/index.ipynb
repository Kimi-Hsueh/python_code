    "import requests\n",
    "\n",
    "url = 'https://tcgbusfs.blob.core.windows.net/dotapp/youbike/v2/youbike_immediate.json'\n",
    "\n",
    "try:\n",
    "    response=requests.request('GET',url)\n",
    "    response.raise_for_status()\n",
    "except:\n",
    "    print('網路下載失敗')\n",
    "else:\n",
    "    if response.status_code == 200:\n",
    "        print('下載成功')"
   ]
